        {% if form.vars.data and form.vars.data.id %}
                <img src="{{ cmf_media_display_url(form.vars.data, { imagine_filter : imagine_filter }) }}" alt="" {% if not imagine_filter %}width="100"{% endif %} />
        {% endif %}
    {% endblock %}
{% endblock %}